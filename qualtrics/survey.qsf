idden":false},"Configuration":{"QuestionDescriptionOption":"UseText","MinSeconds":"0","MaxSeconds":"0"},"QuestionDescription":"Timing","Choices":{"1":{"Display":"First Click"},"2":{"Display":"Last Click"},"3":{"Display":"Page Submit"},"4":{"Display":"Click Count"}},"GradingData":[],"Language":[],"NextChoiceId":24,"NextAnswerId":1,"QuestionID":"QID59"}},{"SurveyID":"SV_5cqznzH1rFpSy46","Element":"SQ","PrimaryAttribute":"QID6","SecondaryAttribute":"What is your Prolific ID? Please note that this response should auto-fill with the correct ID.","TertiaryAttribute":null,"Payload":{"QuestionText":"What is your Prolific ID? Please note that this response should auto-fill with the correct ID.","DefaultChoices":{"TEXT":{"Text":"${e:\/\/Field\/PROLIFIC_PID}"}},"DataExportTag":"ProlificID","QuestionType":"TE","Selector":"SL","DataVisibility":{"Private":false,"Hidden":false},"Configuration":{"QuestionDescriptionOption":"UseText","InputWidth":258,"InputHeight":28},"QuestionDescription":"What is your Prolific ID? Please note that this response should auto-fill with the correct ID.","Validation":{"Settings":{"ForceResponse":"ON","ForceResponseType":"ON","Type":"None","MinChars":"4"}},"GradingData":[],"Language":[],"NextChoiceId":4,"NextAnswerId":1,"SearchSource":{"AllowFreeResponse":"false"},"QuestionID":"QID6"}},{"SurveyID":"SV_5cqznzH1rFpSy46","Element":"SQ","PrimaryAttribute":"QID7","SecondaryAttribute":" ","TertiaryAttribute":null,"Payload":{"QuestionText":"<span style=\"display:none\"><\/span>","DefaultChoices":false,"DataExportTag":"Captcha","QuestionType":"Captcha","Selector":"V2","DataVisibility":{"Private":false,"Hidden":false},"Configuration":{"QuestionDescriptionOption":"UseText","Align":"center","Theme":"light"},"QuestionDescription":" ","GradingData":[],"Language":[],"NextChoiceId":4,"NextAnswerId":1,"QuestionID":"QID7"}},{"SurveyID":"SV_5cqznzH1rFpSy46","Element":"SQ","PrimaryAttribute":"QID8","SecondaryAttribute":"Please first confirm that you are human.","TertiaryAttribute":null,"Payload":{"QuestionText":"Please first confirm that you are human.","DefaultChoices":false,"DataExportTag":"Captcha_Text","QuestionType":"DB","Selector":"TB","DataVisibility":{"Private":false,"Hidden":false},"Configuration":{"QuestionDescriptionOption":"UseText"},"QuestionDescription":"Please first confirm that you are human.","ChoiceOrder":[],"Validation":{"Settings":{"Type":"None"}},"GradingData":[],"Language":[],"NextChoiceId":4,"NextAnswerId":1,"QuestionID":"QID8","QuestionJS":"Qualtrics.SurveyEngine.addOnload(function()\n{\n\t\n\t\/\/ prepare text for comparison section later\n\t\n\t\/\/ function to shuffle arrays from:\n\t\/\/ https:\/\/stackoverflow.com\/questions\/2450954\/how-to-randomize-shuffle-a-javascript-array\n\tfunction shuffle(array) {\n\t\tlet currentIndex = array.length;\n\t\t\/\/ while there remain elements to shuffle...\n\t\twhile (currentIndex != 0) {\n\t\t\t\/\/ pick a remaining element...\n\t\t\tlet randomIndex = Math.floor(Math.random() * currentIndex);\n\t\t\tcurrentIndex--;\n\t\t\t\/\/ and swap it with the current element.\n\t\t\t[array[currentIndex], array[randomIndex]] = [array[randomIndex], array[currentIndex]];\n\t\t}\n\t}\n\t\n\t\/\/ randomly shuffle advisors and save to embedded data\n\tvar advisors = [\n\t\t\"ConsistentlyDeontological\",\n\t\t\"ConsistentlyUtilitarian\",\n\t\t\"NormativelySensitive\",\n\t\t\"NonNormativelySensitive\"\n\t];\n\tshuffle(advisors);\n\tQualtrics.SurveyEngine.setEmbeddedData('AdvisorA', advisors[0]);\n\tQualtrics.SurveyEngine.setEmbeddedData('AdvisorB', advisors[1]);\n\tQualtrics.SurveyEngine.setEmbeddedData('AdvisorC', advisors[2]);\n\tQualtrics.SurveyEngine.setEmbeddedData('AdvisorD', advisors[3]);\n\t\n\t\n\t\/\/ function to produce correct text\n\tfunction produce_text(advisor, advisor_type, advisor_colour, dilemma) {\n\t\tlet text = \"\";\n\t\tlet utilitarian_option = \"\";\n\t\tlet morally_irrelevant_change = \"\";\n\t\tlet morally_relevant_change = \"\";\n\t\t\/\/ iterate over dilemmas\n\t\tif (dilemma == \"Bomb\") {\n\t\t\t\n\t\t\tutilitarian_option = \"kill the employee\";\n\t\t\tmorally_irrelevant_change = \"the name changed from Julie to James\";\n\t\t\tmorally_relevant_change = \"the number of people to save changed from 5 to 1000\";\n\t\t\t\n\t\t} else if (dilemma == \"EnemySpy\") {\n\t\t\t\n\t\t\tutilitarian_option = \"torture the spy's brother\";\n\t\t\tmorally_irrelevant_change = \"the name changed from Frank to Fred\";\n\t\t\tmorally_relevant_change = \"the number of people to save changed from 5 to 1000\";\n\t\t\t\n    \t} else if (dilemma == \"Hostage\") {\n\t\t\t\n\t\t\tutilitarian_option = \"kill the person in the crowd\";\n\t\t\tmorally_irrelevant_change = \"the location changed from a government building to a city bank\";\n\t\t\tmorally_relevant_change = \"the number of hostages to save changed from 5 to 500\";\n\t\t\t\n    \t} else if (dilemma == \"Donation\") {\n\t\t\t\n\t\t\tutilitarian_option = \"donate to the Against Malaria Foundation\";\n\t\t\tmorally_irrelevant_change = \"the person changed from middle-aged to elderly\";\n\t\t\tmorally_relevant_change = \"the number of preventable cases of malaria changed from 5 to 500\";\n\t\t\t\n    \t} else if (dilemma == \"Marathon\") {\n\t\t\t\n\t\t\tutilitarian_option = \"donate to Clean Water For All\";\n\t\t\tmorally_irrelevant_change = \"the location of the marathon changed from London to New York City\";\n\t\t\tmorally_relevant_change = \"the number of preventable cases of waterborne disease changed from 5 to 500\";\n\t\t\t\n    \t} else if (dilemma == \"Volunteering\") {\n\t\t\t\n\t\t\tutilitarian_option = \"volunteer at the homeless shelter\";\n\t\t\tmorally_irrelevant_change = \"the operation changed from a leg operation to a hip operation\";\n\t\t\tmorally_relevant_change = \"the number of homeless people in the shelter changed from 5 to 500\";\n\t\t\t\n\t\t}\n\t\t\/\/ iterate over advisor types\n\t\tif (advisor_type == \"ConsistentlyDeontological\") {\n\t\t\t\n\t\t\ttext = \"<b style='color: \" + advisor_colour + \"'>\" + advisor + \n\t\t\t\t\"<\/b> always chose <i>not<\/i> to \" + utilitarian_option + \".\"\n\t\t\t\n\t\t} else if (advisor_type == \"ConsistentlyUtilitarian\") {\n\t\t\t\n\t\t\ttext = \"<b style='color: \" + advisor_colour + \"'>\" + advisor + \n\t\t\t\t\"<\/b> always chose to \" + utilitarian_option + \".\"\n\t\t\t\n\t\t} else if (advisor_type == \"NormativelySensitive\") {\n\t\t\t\n\t\t\ttext = \"<b style='color: \" + advisor_colour + \"'>\" + advisor + \n\t\t\t\t\"<\/b> initially chose <i>not<\/i> to \" + utilitarian_option + \", but chose to \" + \n\t\t\t\tutilitarian_option + \" when \" + morally_relevant_change + \".\";\n\t\t\t\n\t\t} else if (advisor_type == \"NonNormativelySensitive\") {\n\t\t\t\n\t\t\ttext = \"<b style='color: \" + advisor_colour + \"'>\" + advisor + \n\t\t\t\t\"<\/b> initially chose <i>not<\/i> to \" + utilitarian_option + \", but chose to \" + \n\t\t\t\tutilitarian_option + \" when \" + morally_irrelevant_change + \".\";\n\t\t\t\n\t\t}\n\t\treturn text;\n\t}\n\t\n\t\/\/ save text as embedded data\n\tvar dilemma = \"${e:\/\/Field\/Dilemma}\";\n\tQualtrics.SurveyEngine.setEmbeddedData('AdvisorA_Text', produce_text(\"Advisor A\", advisors[0], \"#E69F00\", dilemma));\n\tQualtrics.SurveyEngine.setEmbeddedData('AdvisorB_Text', produce_text(\"Advisor B\", advisors[1], \"#009E73\", dilemma));\n\tQualtrics.SurveyEngine.setEmbeddedData('AdvisorC_Text', produce_text(\"Advisor C\", advisors[2], \"#0072B2\", dilemma));\n\tQualtrics.SurveyEngine.setEmbeddedData('AdvisorD_Text', produce_text(\"Advisor D\", advisors[3], \"#CC79A7\", dilemma));\n\n});\n\nQualtrics.SurveyEngine.addOnReady(function()\n{\n\t\/*Place your JavaScript here to run when the page is fully displayed*\/\n\n});\n\nQualtrics.SurveyEngine.addOnUnload(function()\n{\n\t\/*Place your JavaScript here to run when the page is unloaded*\/\n\n});"}},{"SurveyID":"SV_5cqznzH1rFpSy46","Element":"SQ","PrimaryAttribute":"QID89","SecondaryAttribute":"Imagine that we gave an advisor the following dilemma and asked them what they think should be do...","TertiaryAttribute":null,"Payload":{"QuestionText":"Imagine that we gave an advisor the following dilemma and asked them what they think should be done. Please read the dilemma carefully.<br><br>\n\n<p style=\"background-color: #FFFFE2; padding: 15px; border: 1px solid grey;\">\n\t<span id=\"dilemma_text_base\"><\/span>\n<\/p><br>\n\nThis is what the advisor said in response to this dilemma:<br><br>\n\n<p style=\"background-color: #F7FEFF; padding: 15px; border: 1px solid grey; text-align: center;\">\"<span id=\"advice_text_base\"><\/span>\"<\/p><br>\n\nOnce you have finished reading, please answer the following questions about this advisor.","DefaultChoices":false,"DataExportTag":"DilemmaBase","QuestionType":"DB","Selector":"TB","DataVisibility":{"Private":false,"Hidden":false},"Configuration":{"QuestionDescriptionOption":"UseText"},"QuestionDescription":"Imagine that we gave an advisor the following dilemma and asked them what they think should be do...","ChoiceOrder":[],"Validation":{"Settings":{"Type":"None"}},"GradingData":[],"Language":[],"NextChoiceId":4,"NextAnswerId":1,"QuestionID":"QID89","QuestionJS":"Qualtrics.SurveyEngine.addOnload(function()\n{\n\t\/\/ get correct text\n\t\n\tvar dilemma = \"${e:\/\/Field\/Dilemma}\";\n\tvar advisor_type = \"${e:\/\/Field\/AdvisorType}\";\n\tvar dilemma_text_base = \"\";\n\tvar advice_text_base = \"\";\n\tif (dilemma == \"Bomb\") {\n\t\t\n\t\tdilemma_text_base = \"Julie works in a small company that employs seven people, including \" + \n\t\t\t\"herself. One morning, a masked and armed man comes into the building with a bomb \" + \n\t\t\t\"strapped to him. He warns that the countdown for the bomb has started and that the \" + \n\t\t\t\"explosion will be large enough to kill everyone in the building. He turns to Julie and offers \" + \n\t\t\t\"a deal. If Julie kills a random colleague, the explosion will be cancelled. Because of the \" + \n\t\t\t\"masked man's gun, Julie knows that if she tries to alert the authorities or attack the man, \" + \n\t\t\t\"she will get shot before she can accomplish anything. The only way for Julie to save the \" + \n\t\t\t\"five other employees is to do as the man says and kill one at random. Should Julie kill one \" + \n\t\t\t\"employee at random in order to save the five others?\"\n\t\tif (advisor_type == \"ConsistentlyDeontological\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Julie should not kill one employee at random to save the ' + \n\t\t\t\t'five others.'\n\t\t} else if (advisor_type == \"ConsistentlyUtilitarian\") {\n\t\t\tadvice_text_base = '<b>Yes<\/b>, Julie should kill one employee at random to save the ' + \n\t\t\t\t'five others.'\n\t\t} else if (advisor_type == \"NormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Julie should not kill one employee at random to save the ' + \n\t\t\t\t'five others.'\n\t\t} else if (advisor_type == \"NonNormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Julie should not kill one employee at random to save the ' + \n\t\t\t\t'five others.'\n\t\t}\n\t\t\n    } else if (dilemma == \"EnemySpy\") {\n\t\t\n        dilemma_text_base = \"Frank is the head of secret intelligence for his country during wartime. He \" + \n\t\t\t\"has captured an enemy spy who can prevent an attack on a small town. The town was recently \" + \n\t\t\t\"evacuated, with five civilians currently still living there. The communication lines with the town \" + \n\t\t\t\"have been cut, meaning that there is no way to warn the people in the town, and the spy is \" + \n\t\t\t\"refusing to reveal the critical information to prevent the attack. Frank also has the spy\u2019s innocent \" + \n\t\t\t\"brother in custody. The only way to extract the critical information from the spy and prevent the \" + \n\t\t\t\"attack is by torturing the spy\u2019s brother. Should Frank torture the spy\u2019s innocent brother in order \" + \n\t\t\t\"to save the five civilians?\"\n\t\tif (advisor_type == \"ConsistentlyDeontological\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Frank should not torture the spy\\'s innocent brother to save the ' + \n\t\t\t\t'five civilians.'\n\t\t} else if (advisor_type == \"ConsistentlyUtilitarian\") {\n\t\t\tadvice_text_base = '<b>Yes<\/b>, Frank should torture the spy\\'s innocent brother to save the ' + \n\t\t\t\t'five civilians.'\n\t\t} else if (advisor_type == \"NormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Frank should not torture the spy\\'s innocent brother to save the ' + \n\t\t\t\t'five civilians.'\n\t\t} else if (advisor_type == \"NonNormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Frank should not torture the spy\\'s innocent brother to save the ' + \n\t\t\t\t'five civilians.'\n\t\t}\n\t\t\n    } else if (dilemma == \"Hostage\") {\n\t\t\n\t\tdilemma_text_base = \"Tom is a hostage negotiator. He is involved in a negotiation with a group \" + \n\t\t\t\"of criminals who have taken five civilians hostage inside a government building. When they \" + \n\t\t\t\"meet in the lobby, the leader of the group makes Tom a deal: he promises to release the five \" + \n\t\t\t\"hostages, but only if Tom kills a random individual from the crowd of onlookers that has \" + \n\t\t\t\"gathered outside the building. Tom knows from his dialogue with the leader that if he doesn\u2019t \" + \n\t\t\t\"comply soon, the group will execute all five hostages without hesitation. Should Tom kill a \" + \n\t\t\t\"random individual in the crowd in order to save the five hostages?\"\n\t\tif (advisor_type == \"ConsistentlyDeontological\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Tom should not kill a random individual in the crowd to save ' + \n\t\t\t    'the five hostages.'\n\t\t} else if (advisor_type == \"ConsistentlyUtilitarian\") {\n\t\t\tadvice_text_base = '<b>Yes<\/b>, Tom should kill a random individual in the crowd to save ' + \n\t\t\t    'the five hostages.'\n\t\t} else if (advisor_type == \"NormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Tom should not kill a random individual in the crowd to save ' + \n\t\t\t    'the five hostages.'\n\t\t} else if (advisor_type == \"NonNormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Tom should not kill a random individual in the crowd to save ' + \n\t\t\t    'the five hostages.'\n\t\t}\n\t\t\n    } else if (dilemma == \"Donation\") {\n\t\t\n\t\tdilemma_text_base = \"Simon is a middle-aged man living in a poor neighbourhood. He is not \" + \n\t\t\t\"wealthy, but throughout the year he always saves some money so that he can give a donation \" + \n\t\t\t\"to charity at Christmas. Simon knows he wants to give to charity, but he is unsure where to \" + \n\t\t\t\"donate. He has narrowed it down to two options. He originally had been thinking about giving \" + \n\t\t\t\"to the 'Against Malaria Foundation', where he knows that his donation equates to preventing \" + \n\t\t\t\"five cases of malaria in Africa. However, a child in his son\u2019s class at school has developed an \" + \n\t\t\t\"incurable disease and lost his eyesight. The second option is therefore to donate the money to \" + \n\t\t\t\"train and provide a guide dog for his son\u2019s friend and classmate. Should Simon donate to the \" + \n\t\t\t\"Against Malaria Foundation to prevent five cases of malaria instead of donating to the local \" + \n\t\t\t\"fundraiser?\"\n\t\tif (advisor_type == \"ConsistentlyDeontological\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Simon should not donate to the Against Malaria Foundation ' + \n\t\t\t    'instead of the local fundraiser.'\n\t\t} else if (advisor_type == \"ConsistentlyUtilitarian\") {\n\t\t\tadvice_text_base = '<b>Yes<\/b>, Simon should donate to the Against Malaria Foundation ' + \n\t\t\t    'instead of the local fundraiser.'\n\t\t} else if (advisor_type == \"NormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Simon should not donate to the Against Malaria Foundation ' + \n\t\t\t    'instead of the local fundraiser.'\n\t\t} else if (advisor_type == \"NonNormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Simon should not donate to the Against Malaria Foundation ' + \n\t\t\t    'instead of the local fundraiser.'\n\t\t}\n\t\t\n    } else if (dilemma == \"Marathon\") {\n\t\t\n\t\tdilemma_text_base = \"Rachel has been training for months to run the London Marathon to \" + \n\t\t\t\"raise money for charity. She has already gathered a substantial amount in donations and is \" + \n\t\t\t\"now deciding where the funds should go. She has two options. The first option is to donate \" + \n\t\t\t\"the money to 'Clean Water for All', an organisation that works to provide clean drinking water \" + \n\t\t\t\"around the world. She knows that her donation equates to preventing five cases of waterborne \" + \n\t\t\t\"disease in impoverished communities. The second option is to donate the money to a local \" + \n\t\t\t\"charity run by a friend whose child has recently been diagnosed with a rare genetic disorder. \" + \n\t\t\t\"The donations would go toward helping fund the expensive treatment that could significantly \" + \n\t\t\t\"improve the quality of life for the child, who is someone Rachel knows and cares about. Should \" + \n\t\t\t\"Rachel donate to Clean Water for All to prevent five cases of waterborne disease instead of \" + \n\t\t\t\"donating to the local charity?\"\n\t\tif (advisor_type == \"ConsistentlyDeontological\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Rachel should not donate to Clean Water For All instead of the ' + \n\t\t\t    'local charity.'\n\t\t} else if (advisor_type == \"ConsistentlyUtilitarian\") {\n\t\t\tadvice_text_base = '<b>Yes<\/b>, Rachel should donate to Clean Water For All instead of the ' + \n\t\t\t    'local charity.'\n\t\t} else if (advisor_type == \"NormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Rachel should not donate to Clean Water For All instead of the ' + \n\t\t\t    'local charity.'\n\t\t} else if (advisor_type == \"NonNormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Rachel should not donate to Clean Water For All instead of the ' + \n\t\t\t    'local charity.'\n\t\t}\n\t\t\n    } else if (dilemma == \"Volunteering\") {\n\t\t\n\t\tdilemma_text_base = \"Janet\u2019s father is feeling really lonely while recovering from an operation \" + \n\t\t\t\"on his leg that means he cannot leave the house. Janet has a week off work, and her father \" + \n\t\t\t\"has asked Janet to come and spend the week with him to keep him company. Janet had \" + \n\t\t\t\"planned on using the week to volunteer at a soup kitchen in a small homeless shelter. By \" + \n\t\t\t\"volunteering, she would be able to provide help to the five homeless people currently living \" + \n\t\t\t\"at the shelter, rather than making her father happier. Should Janet volunteer to help the five \" + \n\t\t\t\"homeless people instead of supporting her father?\"\n\t\tif (advisor_type == \"ConsistentlyDeontological\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Janet should not volunteer instead of supporting her father.';\n\t\t} else if (advisor_type == \"ConsistentlyUtilitarian\") {\n\t\t\tadvice_text_base = '<b>Yes<\/b>, Janet should volunteer instead of supporting her father.';\n\t\t} else if (advisor_type == \"NormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Janet should not volunteer instead of supporting her father.';\n\t\t} else if (advisor_type == \"NonNormativelySensitive\") {\n\t\t\tadvice_text_base = '<b>No<\/b>, Janet should not volunteer instead of supporting her father.';\n\t\t}\n\t\t\n\t}\n\t\n\t\/\/ pipe text into html\n\tQualtrics.SurveyEngine.setEmbeddedData( 'Advice_Base', advice_text_base);\n\tdocument.getElementById('dilemma_text_base').innerHTML = dilemma_text_base;\n\tdocument.getElementById('advice_text_base').innerHTML = advice_text_base;\n\n});\n\nQualtrics.SurveyEngine.addOnReady(function